pipeline {
    agent any
    stages {
       stage("Delete all resources") {
          steps {
             echo "Hello! Pipeline is running!"
             script {
                openshift.withCluster() {
                   openshift.withProject() {
                      openshift.selector("all", "test-app").delete() 
                     }   
                    }
                 }  
             }
          }
       }
    }
